{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "semicolon": "off"
    },
    "eslintIgnore": [
        "*.test.js",
        "*.spec.js"
    ]
}
